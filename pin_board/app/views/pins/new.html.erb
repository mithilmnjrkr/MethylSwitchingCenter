<div align="center">
  <h3>Create new pin</h3>

  <%= simple_form_for Pin.new, :html => {multipart:true} do |f| %>
    <%= f.input :title , :required => true %>
    <%= f.input :description , :required => true %>
    <div>
      </br>
      <%= f.button :submit, 'Create', :class=> "btn btn-primary" %>
    </div>
  <% end %>
  <%= link_to "Back", root_path%>
</div>


